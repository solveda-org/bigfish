<Content contentId="SI_SEARCH" statusId="CTNT_PUBLISHED"/>
<ElectronicText dataResourceId="SI_SEARCH">
  <textData><![CDATA[
<script type="text/javascript">
function clearIt(what)
{
if(what.value == what.defaultValue) what.value = '';
}
function setIt(what)
{
if(what.value == '') what.value = what.defaultValue;
}
</script>

<form method="get" id="frmSearchForm" name="frmSearchForm" action="siteSearch" onsubmit="return submitSearchForm(document.frmSearchForm);">
    <fieldset title="Search this site..." class="formstyle">
        <!-- <label for="searchText">${uiLabelMap.SearchCaption}</label> -->
        <span id="searchInputContainer">
            <input type="text" name="searchText" class="input placeholder" id="searchText" value="${StringUtil.wrapString(SEARCH_DEFAULT_TEXT!)}" maxlength="60" onfocus="clearIt(this)" onblur="setIt(this)" />
            <input type="image" src="/osafe_theme/images/user_content/images/search-field-bg-2.png" name="searchBtn" value="${uiLabelMap.HeaderSearchBtn}" />
        </span>
    </fieldset>
</form>]]>
</textData>
</ElectronicText>

