<Content contentId="SI_SHOPPING_CART" statusId="CTNT_PUBLISHED"/>
<ElectronicText dataResourceId="SI_SHOPPING_CART">
  <textData><![CDATA[<a href="<@ofbizUrl>eCommerceShowcart</@ofbizUrl>">
       <span id="shoppingCartCaption">${uiLabelMap.ShoppingCartCaption}</span>
        <div id="siteCartStats">
            <div class="highlightGroup">
                <img id="viewCartSmallImage" alt="View Cart" src="/osafe_theme/images/cart/viewCart_small.gif">
                <span id="siteShoppingCartSize">${shoppingCartSize} Items</span> <span class="price"><@ofbizCurrency amount=shoppingCart.getSubTotal() isoCode=CURRENCY_UOM_DEFAULT!shoppingCart.getCurrency()/></span>
            </div>
        </div>
        <#--<input type="button" id="viewCartButtonImage" name="viewCartButtonImage" value=""  />-->
        <input type="button" id="viewCartButton" class="standardBtn action" name="viewCartButton" value="${uiLabelMap.ViewCartBtn}"  />
</a>]]>
  </textData>
</ElectronicText>

